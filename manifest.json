{
   "name": "Fast Note",
   "version": "1.2.0",
   "description": "Super fast note storage",
   "manifest_version": 2,
   "background": {
     "scripts": [
       "js/background.js"
       ]
   },
   "browser_action": {
      "default_icon": "logo48.png",
      "default_popup": "popup.html",
      "default_title": "Fast Note"
   },
   "icons": {
      "128": "logo128.png",
      "48": "logo48.png"
   },
   "permissions": [ "background", "unlimitedStorage" ]
}
